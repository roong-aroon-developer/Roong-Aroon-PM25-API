{
    "version": 2,
    "name": "Roongaroon-dust-detector",
    "builds": [{
        "src": "app.js",
        "use": "@now/node-server",
        "config": { "maxLambdaSize": "50mb" }
    }],
    "routes": [{
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "GET",
            "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
        },
        "src": "/.*",
        "dest": "/app.js"
    }],
    "alias": "ra-dust.now.sh",
    "github": {
        "enabled": false
    }
}